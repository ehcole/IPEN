#!/bin/sh
#### PBS preamble
#PBS -N 4-2_out

#PBS -M ehcole@umich.edu
#PBS -m a

#PBS -l procs=16,walltime=03:00:00 
#PBS -V

#PBS -A wrm_fluxoe
#PBS -q fluxoe
#PBS -l qos=flux
#### End PBS preamble

if [ -n "$PBS_O_WORKDIR" ]; then cd $PBS_O_WORKDIR; fi

react2xml.pl 4-2-1.inp ../../../../../outputs/2D/out/4-2-1_out.xml
mpirun MPACT.exe ../../../../../outputs/2D/out/4-2-1_out.xml

react2xml.pl 4-2-2.inp ../../../../../outputs/2D/out/4-2-2_out.xml
mpirun MPACT.exe ../../../../../outputs/2D/out/4-2-2_out.xml

react2xml.pl 4-2-3.inp ../../../../../outputs/2D/out/4-2-3_out.xml
mpirun MPACT.exe ../../../../../outputs/2D/out/4-2-3_out.xml
